<hibernate-mapping>
<class name="hibernate.basic.UserDetails" table="user_test">

<id name="userId" column="user_id"  >
<generator class="increment" />
</id>

<property name="userName" column="user_name" length="10"/>


</class>
</hibernate-mapping>